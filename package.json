{
  "name": "gpt-task-runner",
  "version": "1.0.0",
  "description": "A powerful task runner and automation tool designed to work with GPT models for intelligent task execution and workflow management.",
  "main": "dist/index.js",
  "bin": {
    "gpt-task-runner": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "start:service": "node dist/service.js",
    "start:dashboard": "node dist/dashboard.js",
    "dev:service": "ts-node src/service.ts",
    "dev:dashboard": "ts-node src/dashboard.ts",
    "dev:all": "concurrently \"npm run dev:service\" \"npm run dev:dashboard\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:dry-run": "node scripts/test-dry-run.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "install-service": "node scripts/install-service.js",
    "uninstall-service": "node scripts/uninstall-service.js"
  },
  "keywords": [
    "gpt",
    "task-runner",
    "automation",
    "openai",
    "ai"
  ],
  "author": "Jay Rosh",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "sqlite3": "^5.1.6",
    "node-windows": "^1.0.0-beta.8",
    "ws": "^8.14.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
